{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"packageManager": "pnpm@8.6.12",
	"scripts": {
		"dev": "vite dev --host 0.0.0.0",
		"docker-dev": "tsx --tsconfig scripts/tsconfig.json scripts/docker-dev.ts",
		"openapi-gen": "tsx --tsconfig scripts/tsconfig.json scripts/openapi-gen.ts",
		"build": "vite build",
		"preview": "vite preview",
		"analyze": "pnpm vite-bundle-visualizer",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier . -l -w --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss"
	},
	"dependencies": {
		"@floating-ui/dom": "^1.5.1",
		"@skeletonlabs/skeleton": "^2.0.0",
		"@skeletonlabs/tw-plugin": "^0.1.0",
		"@tanstack/svelte-query": "5.0.0-beta.20",
		"autoprefixer": "^10.4.15",
		"openapi-fetch": "^0.7.4",
		"postcss": "^8.4.28",
		"svelte-local-storage-store": "^0.6.0",
		"tailwindcss": "^3.3.3"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.0.2",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.23.0",
		"@types/node": "^20.5.6",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.47.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.33.0",
		"nodemon": "^3.0.1",
		"openapi-typescript": "^6.5.3",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.0.2",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.3",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.0",
		"tslib": "^2.6.2",
		"tsx": "^3.12.7",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vite-bundle-visualizer": "^0.10.0"
	},
	"pnpm": {
		"patchedDependencies": {
			"openapi-fetch@0.7.4": "patches/openapi-fetch@0.7.4.patch",
			"@sveltejs/kit@1.23.0": "patches/@sveltejs__kit@1.23.0.patch",
			"@skeletonlabs/skeleton@2.0.0": "patches/@skeletonlabs__skeleton@2.0.0.patch"
		}
	}
}
